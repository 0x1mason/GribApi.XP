    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <dependencies>
      <dependency id="vcredist2013" version="12.0.30501.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
